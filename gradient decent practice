        "y_predicted = np.array([1, 1, 0, 0, 1])\n",
        "y_true = np.array([0.3, 0.2, 0.1, 0.4, 0.2])"
      ],
      "metadata": {
        "id": "kwzAjXQhtSdv"
      },
      "execution_count": 5,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def mae(y_true, y_predicted):\n",
        "  total_err = 0\n",
        "  for yt, yp in zip(y_true, y_predicted):\n",
        "    total_err += abs(yt-yp)\n",
        "  mae = total_err /len(y_true)\n",
        "  return total_err, round(mae, ndigits=2)"
      ],
      "metadata": {
        "id": "lkrKuL5FtbHP"
      },
      "execution_count": 27,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "total_err, mae_err = mae(y_true, y_predicted)\n",
        "print('total err: ', total_err)\n",
        "print('mae err: ', mae_err)"
      ],
      "metadata": {
        "colab": {
